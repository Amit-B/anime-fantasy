@f(_,convert.playerid(str[],user))
{
	new playerid = INVALID_PLAYER_ID;
	if(str_isnum(str)) playerid = strval(str);
	else
	{
		if((str[0] == '@' || str[0] == '#') && user != INVALID_PLAYER_ID)
		{
			if(str[1] == 'm') playerid = user;
			else if(str[1] == 'r') playerid = player_GetRandom();
			else playerid = INVALID_PLAYER_ID;
		}
		else
		{
			LoopEx(player,i,<playerid == INVALID_PLAYER_ID>) if(equal(str,player_GetName(i)) || equal(str,player_GetNickname(i))) playerid = i;
			if(playerid == INVALID_PLAYER_ID) LoopEx(player,i,<playerid == INVALID_PLAYER_ID>) if(strfind(str,player_GetName(i)) != -1 || strfind(str,player_GetNickname(i)) != -1) playerid = i;
		}
	}
	return playerid;
}
@f(bool,convert.boolean(num)) return bool:clamp(num,0,1);
@f(_,convert.secondsformat(time,second = 100))
{
	new s[8], ss = time % second;
	if(!ss) format(s,sizeof(s),"%ds",time / second);
	else
	{
		while(ss % 10 == 0 && ss > 9) ss /= 10;
		format(s,sizeof(s),"%d.%ds",time / second,ss);
	}
	return s;
}
@f(Float,convert.coordsarray(Float:minx=0.0,Float:miny=0.0,Float:minz=0.0,Float:maxx=0.0,Float:maxy=0.0,Float:maxz=0.0))
{
	new Float:coords[6];
	coords[0] = minx, coords[1] = miny, coords[2] = minz, coords[3] = maxx, coords[4] = maxy, coords[5] = maxz;
	return coords;
}
